{
  "name": "kefir-sampler",
  "version": "0.0.1",
  "description": "Create a function that samples related stream",
  "main": "lib/index.js",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git@github.com:jeron-diovis/kefir-sampler.git"
  },
  "scripts": {
    "test": "mocha --harmony --recursive --compilers js:babel-register",
    "test:watch": "npm test -- --watch",
    "lint": "esw src",
    "lint:watch": "npm run lint -- -w",
    "start": "parallelshell 'npm run test:watch' 'npm run lint:watch'",
    "clean": "rm -rf dist lib",
    "prebuild": "npm run lint && npm test && npm run clean",
    "build": "babel src -d lib"
  },
  "keywords": [
    "kefir",
    "reactive",
    "stream"
  ],
  "author": "Jeron Diovis",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-watch": "^2.1.10",
    "kefir": "^3.2.1",
    "mocha": "^2.4.5",
    "parallelshell": "^2.0.0",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "kefir-bus": "^2.1.0"
  }
}
